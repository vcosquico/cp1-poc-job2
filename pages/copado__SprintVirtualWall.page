            function sortTable(f,n){
              var rows = $copado('#mainTable tbody  tr').get();

              rows.sort(function(a, b) {
                             
                var A = getVal(f,a);
                var B = getVal(f,b);
                if(A < B) {
                  return -1*f;
                }
                if(A > B) {
                  return 1*f;
                }
                return 0;
              });
              
              function getVal(f,elm){
                var v;
                if($copado(elm).children('td').eq(n)[0]){
                  var element = $copado(elm).children('td').eq(n)[0].querySelectorAll("span[customClass='output']");
                  
                  if(element.length > 0){
                    v = element[0].textContent.toUpperCase();;
                  }else{
                    var aelement = $copado(elm).children('td').eq(n)[0].querySelectorAll("a");
                    if(aelement.length > 0){
                      v = aelement[0].textContent.toUpperCase();;
                    }else{
                      v = "";
                    }
                  }
                }else{
                  v = "";
                }
                
                if($copado.isNumeric(v)){
                  v = parseInt(v,10);
                }
                return v;
              }

              $copado.each(rows, function(index, row) {
                $copado('#mainTable').children('tbody').append(row);
              });
            }
            var f_sl = 1;
            var sortEnabled = true;
            function sortGiven(elem,allowDrag){
                if(f_sl == 1){
                  $copado(elem).closest('th').addClass('slds-is-sorted slds-is-sorted_desc');                
                  $copado(elem).closest('th').removeClass('slds-is-sorted slds-is-sorted_asc');                
                }else{
                  $copado(elem).closest('th').addClass('slds-is-sorted slds-is-sorted_asc');                
                  $copado(elem).closest('th').removeClass('slds-is-sorted slds-is-sorted_desc');                                  
                }
                
                if(allowDrag == true && f_sl == -1 && taskView == false){
                  $copado("#mainTableBody").sortable("enable");
                  sortEnabled = true;
                  $copado("#sortImage").show();
                }else{
                  $copado("#mainTableBody").sortable("disable");
                  sortEnabled = false;
                  $copado("#sortImage").hide();
                }
                f_sl *= -1;
                var n = $copado(elem).prevAll().length;
                sortTable(f_sl,n);
            }

            calculateWidth = function(elem, e) {
                var childObj = $copado(elem).children().first();
                var parObj = childObj.parents().first();
                var count = 1;
                
                while(parObj.prop("tagName") != 'TH') {
                    parObj = parObj.parents().first();
                    count++;
                }

                var mouseStart=e.clientX;
                mStart = mouseStart;
                oWidth = parObj.outerWidth();
            };
             
            setNewWidth = function(elem, e) {
                var childObj = $copado(elem).children().first();
                var parObj = childObj.parents().first();
                var count = 1;
                
                while(parObj.prop("tagName") != 'TH') {
                    parObj = parObj.parents().first();
                    count++;
                }

                var mouseStart = mStart;
                var oldWidth = oWidth
                
                if(e.clientX > 0){
                    var newWidth = e.clientX- parseFloat(mouseStart)+parseFloat(oldWidth);
                    parObj.width(newWidth);
                }
            }
            function calcFilters(){
                //see if at least on filter is checked
                var nothingSelected = true;
                $copado('.item-checkbox').each(function() {
                  if($copado(this).prop("checked")){
                    nothingSelected = false;
                  }
                });     
                //Hide all rows
                $copado('#mainTable tr:not(:first)').each(function() {
                  $copado(this).hide(); 
                });           
                //If there is one or more items selected then rows are added
                
                if(nothingSelected == true){
                  $copado('#mainTable tr:not(:first)').each(function() {
                    $copado(this).show(); 
                   });    
                }else{
                  var assigneeSet = {};
                  var rtSet = {};
                  var statusSet = {};                  
                  $copado('.item-checkbox').each(function() {
                    var jqElem = ($copado(this));
                    var ch = jqElem.prop("checked");
                    var filterValue = jqElem.parent().parent().find("div.itemTitle").find("label").text();
                    var filterType = jqElem.closest("#filterRecordTypes").find("h1").text();
                    if(filterType == 'Assignee' && ch){
                      assigneeSet[filterValue] = true;
                    }else if(filterType == 'Record Type' && ch){
                      rtSet[filterValue] = true;
                    }else if(filterType == 'Status' && ch){
                      statusSet[filterValue] = true;
                    }
                  });      
                  
                  $copado('#mainTable tr:not(:first)').each(function(){
                        var aelement = $copado(this).find('#devTD')[0];
                        var rtElement = $copado(this).find('#rtTD')[0];
                        if(rtElement && rtElement.querySelectorAll("span[customClass='output']")){
                          var rtName = (rtElement.querySelectorAll("span[customClass='output']"))[0].textContent;
                        }
                        var stElement = $copado(this).find('#statTD')[0];
                        if(stElement && stElement.querySelectorAll("span[customClass='output']")){
                          var stName = (stElement.querySelectorAll("span[customClass='output']"))[0].textContent;
                        }
                        if(aelement &&  aelement.querySelectorAll("a")){
                          var aName = aelement.querySelectorAll("a")[0].textContent;
                          var statusSetLength = Object.keys(statusSet).length;
                          var assigneeSetLength = Object.keys(assigneeSet).length;
                          var rtSetLength = Object.keys(rtSet).length;


                          if((assigneeSet[aName] && rtSet[rtName] && statusSet[stName]) ||
                            (assigneeSet[aName] && !rtSetLength && !statusSetLength)||
                            (rtSet[rtName] && !assigneeSetLength && !statusSetLength)||
                            (statusSet[stName] && !assigneeSetLength && !rtSetLength)||
                            (assigneeSet[aName] && rtSet[rtName] && !statusSetLength)||
                            (assigneeSet[aName] && statusSet[stName] && !rtSetLength)||
                            (rtSet[rtName] && statusSet[stName] && !assigneeSetLength)
                            ){
                              $copado(this).show();
                          }                   
                        }         
                    });                  
                }
                calcVisibleRows();
              }
              function calcVisibleRows(){
                var rowCounter = 0;
                $copado('#mainTable tr:not(:first)').each(function() {
                  if($copado(this).is(':visible')){
                    rowCounter = rowCounter +1;  
                  }
                  
                 });                  
                $copado("#filterResults").text('Showing '+rowCounter+' User Stories');
              }

